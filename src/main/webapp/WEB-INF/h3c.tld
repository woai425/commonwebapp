<taglib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
	version="2.0">
	<description>H3C标签库</description>
	<tlib-version>1.0</tlib-version>
	<short-name>h3c</short-name>

	<tag>
		<description>accordionMenu界面左侧树形导航菜单、后台树形菜单</description>
		<name>accordionMenu</name>
		<tag-class>com.h3c.portal.taglib.menu.AccordionMenuTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>获取导航菜单数据,接收类型为List,可以使用el表达式注入,例如${menus},data和dataurl二者选填一个</description>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>获取导航菜单数据的路径，通过ajax方式获取，接收数据类型为json数组对象，data和dataurl二者选填一个</description>
			<name>dataurl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单风格，如果usage为back,style有两种1.file 2.sign;如果usage为portal,style有四种1.blueFlat 2.blackFlat 3.whiteFlat 4.whiteNoFlat</description>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单字体大小，比如：15px,这里需要加上“px”单位</description>
			<name>fontsize</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单整体宽度,比如：300px,这里需要加上“px”单位</description>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>handler用户自定义js函数，参数有两个functionName(id,url)</description>
			<name>handler</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单整体高度</description>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单是用在portal还是用在后台管理。如果用于portal左侧导航栏填写"portal",如果用户后台管理填写"back"</description>
			<name>usage</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>主体Iframe的Id值</description>
			<name>contentIframeId</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>导航栏的ID</description>
			<name>property</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>treeMenu后台树形导航菜单</description>
		<name>treeMenu</name>
		<tag-class>com.h3c.portal.taglib.menu.TreeMenuTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>获取树形菜单数据的路径，通过ajax方式获取，接收数据类型为json数组对象，data和dataurl二者选填一个</description>
			<name>dataurl</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单字体大小，比如：15px,这里需要加上“px”单位</description>
			<name>fontsize</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单整体宽度,比如：300px,这里需要加上“px”单位</description>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>handler用户自定义js函数，参数有两个functionName(id,url)</description>
			<name>handler</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>菜单整体高度</description>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>导航栏的ID</description>
			<name>property</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>tab的容器，里面放置tabTitle和tabContent，可以统一调整tabContent样式</description>
		<name>tab</name>
		<tag-class>com.h3c.portal.taglib.tab.TabTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>property，tabContent容器的id</description>
			<name>property</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>控制tabContent容器的宽度</description>
			<name>width</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>控制tabContent容器的高度</description>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>tab的标题数据源，格式如[id1,title1],[id2,title2],……，id为tabContent的id，title自定义</description>
			<name>data</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>标题点击事件，事件传入的值为tabContent的id和标题名</description>
			<name>handler</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>内容背景颜色，如：#ffffff</description>
			<name>contentbgcolor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>标题背景颜色，如：#ffffff</description>
			<name>titlebgcolor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>标题打开时的背景颜色，如：#ffffff</description>
			<name>activecolor</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>初始化时打开的标题，值为想要初始化时就打开的tabContent的id</description>
			<name>activeTab</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>每个tab的内容</description>
		<name>tabContent</name>
		<tag-class>com.h3c.portal.taglib.tab.TabContentTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>tabContent的id,id跟tabTitle中data数据中的id关联,具体关联方式可以参照tabTitle标签的提示信息</description>
			<name>property</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>true:页面初始化时打开</description>
			<name>active</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>导入要用到的头文件(样式及js文件)</description>
		<name>head</name>
		<tag-class>com.h3c.portal.taglib.html.HeadTag</tag-class>
		<body-content>empty</body-content>
	</tag>

	<tag>
		<description>页面上的自定义table布局标签</description>
		<name>tabLayOut</name>
		<tag-class>com.h3c.portal.taglib.html.TabLayOutTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
